    "gulp-gzip": "^1.4.2",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-inject": "^5.0.2",
    "gulp-inject-string": "^1.1.2",
    "gulp-jscs": "^4.1.0",
    "gulp-jshint": "^1.9.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify": "^3.1.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-rev-all": "^1.0.0",
