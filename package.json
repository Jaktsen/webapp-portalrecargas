{
  "name": "webapp-portalrecargas-angular8",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "gulp clean && ng build --prod --extract-css=false --aot=true --buildOptimizer=true && gulp publishUnique --env dev",
    "auth": "gulp clean && ng build --prod --extract-css=false --aot=true --buildOptimizer=true && gulp publishUnique --env auth",
    "tar": "gulp clean && ng build --prod --extract-css=false && gulp maketarUnique",
    "cov": "ng test --no-watch --code-coverage",
    "tc": "ng test --code-coverage",
    "pubm": "gulp clean && ng build --prod --sourceMap=true && gulp publish",
    "authm": "gulp clean && ng build --prod --sourceMap=true && gulp auth",
    "pre-commit": "ng build --prod --extract-css=false",
    "pre-push": "ng build --prod --extract-css=false && ng test --no-watch",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-point"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": "npm run pre-push"
    }
  },
  "private": true,
  "dependencies": {
