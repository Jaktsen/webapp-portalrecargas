    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@popperjs/core": "^2.0.0-rc.3",
    "bootstrap": "^4.4.1",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "ng5-auto-complete": "0.0.8",
    "node-gyp": "^6.1.0",
    "popper.js": "^1.16.1",
    "radium": "^0.26.2",
    "rebuild": "^0.1.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.14.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
